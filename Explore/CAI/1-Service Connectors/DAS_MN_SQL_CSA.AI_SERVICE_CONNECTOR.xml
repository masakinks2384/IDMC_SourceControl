                  <binding>
                     <sqlStatement statementId="SQL Name 1">
                        <statement>SELECT [ID]
      ,[ACCOUNTID]
      ,[LASTNAME]
      ,[FIRSTNAME]
      ,[OTHERSTREET]
      ,[EMAIL]
  FROM [CSA].[dbo].[SF_CONTACT] where LASTNAME = {$i_LASTNAME}</statement>
